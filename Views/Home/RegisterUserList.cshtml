@model List<CompleteRegisterViewmodel>
@section scripts {
    <script type="text/javascript">
    </script>
}
<div class="container">
    <h1 class="m-4">Registered Users</h1>
    <table class="table table-hover">
        <thead>
            <tr>
                <th>Username</th>
                <th>Date Of Registration</th>
                <th>Full Name</th>
                <th>Membership Type</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var x in Model)
            {
                <tr>
                    <td>@x.Username</td>
                    <td>@x.RegisterDate</td>
                    <td>@x.CustomerName</td>
                    <td>
                        @if (x.SubscriptionType == 1)
                        {

                            @("30 Day Free Trial")
                        }

                        @if (x.SubscriptionType == 2)
                        {
                            @("Monthly Membership")
                        }
                        @if (x.SubscriptionType == 3)
                        {

                            @("Three-Month Membership")
                        }

                        @if (x.SubscriptionType == 4)
                        {
                            @("Annual Membership")
                        }
                </td>

            </tr>
            }

        </tbody>
    </table>
</div>

